
public class Test {

	public static void main(String[] args) {
		Test test = new Test();
		
		System.out.println(test.sqrt(250));
	}
	
	public static double sqrt(int A) {

		double i = 1, j = A / 2;
		while (i <= j) {
			if (A - i * i <= 0.01) {
				return i;
			}
			i = i + 0.01;
		}
		return i;
	}
} 
